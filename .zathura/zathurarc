# Colors
set recolor-lightcolor      "#282828"
set recolor-darkcolor       "#ccaa69" #"#fbf1c7"
set recolor                 "false"

# Keeps original colors if set
set recolor-keephue         "false"

# Rest of configs
set selection-clipboard clipboard
set smooth-scroll true
set statusbar-home-tilde

map <C-r> rotate
map r recolor
map <S-Up> zoom in
map <S-Down> zoom out
